<%= form_with url: new_session_path, scope: :session, local: true do |form| %>
    <%= form.label :email %>
    <%= form.email_field :email, class: 'mb-1 form-control' %>

    <%= form.label :password %>
    <%= form.password_field :password, class: 'mb-1 form-control' %>

    <%= form.submit "Log In", class: 'btn btn-primary' %>
<% end %>
